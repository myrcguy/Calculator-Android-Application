            android:layout_height="40dp" android:id="@+id/buttonPM"
            app:layout_constraintEnd_toStartOf="@+id/buttonLN"
            android:layout_marginEnd="116dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="280dp" android:text="+/-" android:backgroundTint="#2196F3"
            android:textSize="18sp"/>
