            android:layout_width="90dp"
            android:layout_height="40dp" android:id="@+id/buttonMinus"
            android:layout_marginEnd="4dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="200dp" android:backgroundTint="#2196F3" android:textSize="18sp"/>
    <Button
            android:text="="
            android:layout_width="90dp"
            android:layout_height="40dp" android:id="@+id/buttonEquals"
            android:layout_marginEnd="4dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="120dp" android:backgroundTint="#BA1616" android:textSize="18sp"/>
